
@incollection{fidali_detection_2018,
	address = {Cham},
	title = {Detection of {Welding} {Process} {Instabilities} {Using} {Acoustic} {Signals}},
	volume = {10},
	isbn = {978-3-319-62041-1 978-3-319-62042-8},
	url = {http://link.springer.com/10.1007/978-3-319-62042-8_18},
	abstract = {Monitoring of welding process is an important issue, especially from the point of view of operation of automated welding systems. One of the source of diagnostic information about welding process is sound generated during welding. The sound generated during the welding process includes acoustic audible and inaudible sounds. During the research author focused on analysis ultrasound part of acoustic energy. Acoustic signals used for research purposes were collected during series of experiments conducted in laboratory conditions. Acoustic signals were gather using condenser microphone with measurement range 20-100kHz. The acquired signals were high pass filtered and analyzed using such statistical features as RMS, IQR and Kurtosis in order to detect welding instabilities. The best results obtained for signals in band 20-140kHz estimated with use of IQR parameter. The results of the research confirms usefulness of ultrasound signals for detection of welding faults.},
	language = {en},
	urldate = {2020-11-24},
	booktitle = {Advances in {Technical} {Diagnostics}},
	publisher = {Springer International Publishing},
	editor = {Fidali, Marek and Łazarz, Bogusław Edward and Chaari, Fakher and Burdzik, Rafał and Timofiejczuk, Anna},
	year = {2018},
	doi = {10.1007/978-3-319-62042-8_18},
	note = {Series Title: Applied Condition Monitoring},
	pages = {191--201},
}

