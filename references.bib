@article{muto_hybrid_2001,
	title = {Hybrid composites with self-diagnosing function for preventing fatal fracture},
	volume = {61},
	issn = {02663538},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0266353800001652},
	doi = {10.1016/S0266-3538(00)00165-2},
	abstract = {Design of intelligent material with self-diagnosing function for preventing fatal fracture of the structural material is achieved by hybrid composites consisting of ®ber tows or powders in carbon material with a small value of ultimate elongation and glass ®ber tows with a large value of ultimate elongation. CPGFRP/GFRP (carbon powder dispersed in glass-®ber-reinforced plastics/glass®ber-reinforced plastics) composites are superior to CFGFRP (carbon-®ber and glass-®ber-reinforced plastics) composites in respect of their capability of damage monitoring by measuring change in electrical resistance from the early stage of damage, but the former is inferior to the latter as an application because of the diculties of mass production and high cost. CFGFRP composites are already commercially used as sensor rods for diagnosing the health of concrete structures in skyscrapers in the architectural ®eld or in security walls for preventing an invader in the security ®eld. A method based on monitoring damage by measuring changes in the electrical resistance of structural materials is promising for improving the reliability of material. \# 2001 Elsevier Science Ltd. All rights reserved.},
	language = {en},
	number = {6},
	urldate = {2020-11-19},
	journal = {Composites Science and Technology},
	author = {Muto, N and Arai, Y and Shin, S.G and Matsubara, H and Yanagida, H and Sugita, M and Nakatsuji, T},
	month = may,
